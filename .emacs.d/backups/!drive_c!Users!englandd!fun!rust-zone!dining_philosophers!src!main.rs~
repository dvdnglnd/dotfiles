
struct Philosopher {
  name: String,
}

impl Philosopher {
  fn new(name: &str) -> Philosopher {
    Philosopher {
      name: name.to_string(),
    }
  }
}

fn main() {
  let p1 = Philosopher::new("Karl Marx");
  let p2 = Philosopher::new("Fredrich Nitze");
  let p3 = Philosopher::new("Emma Goldman");
  let p4 = Philosopher::new("Tim Keller");
  let p5 = Philosopher::new("Michel Foucalt");
}
